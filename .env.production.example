# Production Environment Variables for YuhHearDem
# Copy this file to .env on the production server

# Application
APP_NAME=YuhHearDem
APP_ENV=production
DEBUG=False
LOG_LEVEL=INFO

# Database - Update connection details as needed
DATABASE_URL=postgresql+asyncpg://postgres:YHD_9f3b7c2d5a8e1f6g@localhost:5432/yuhheardem
DATABASE_POOL_SIZE=20
DATABASE_MAX_OVERFLOW=10

# Google Gemini API - Add your production API key
GOOGLE_API_KEY=your_production_gemini_api_key_here
GEMINI_MODEL=gemini-3-flash-preview
GEMINI_TEMPERATURE=0.3

# Vector Embeddings
EMBEDDING_MODEL=all-MiniLM-L6-v2
EMBEDDING_DIMENSIONS=384

# spaCy
SPACY_MODEL=en_core_web_trf

# Fuzzy Matching
FUZZY_MATCH_THRESHOLD=85

# Cache
CACHE_TTL_SECONDS=3600
